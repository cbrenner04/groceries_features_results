<h1>Results for "<%= @feature.description %>"</h1>
<%= link_to "Back to Features", features_path %>

<table style="width:100%">
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Duration</th>
      <th>Exception</th>
      <th>Passed?</th>
      <th>Environment</th>
    </tr>
  </thead>
  <tbody>
    <%= render @results %>
  </tbody>
</table>
